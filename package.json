{
  "name": "express-boilerplate-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node build/index.js",
    "dev": "nodemon src/index.ts",
    "db:generate": "npx prisma generate --schema ./src/config/prisma",
    "db:migrate": "npx prisma migrate dev --name init --schema ./src/config/prisma/schema.prisma",
    "db:generate:schema": "npx prisma generate --schema ./src/config/prisma/schema.prisma",
    "test:db": "npx prisma db push --schema=./src/config/prisma/schema.prisma",
    "test": "NODE_ENV=test jest --runInBand",
    "test:full": "npm run test:db && npm run test",
    "worker:play": "npx ts-node src/workers/email/email.worker.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^6.10.0",
    "@bull-board/express": "^6.10.0",
    "@prisma/client": "^6.1.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "bullmq": "^5.53.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.6.1",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@10.11.0"
}
